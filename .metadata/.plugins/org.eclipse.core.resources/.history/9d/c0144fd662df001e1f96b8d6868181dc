package Ch12Ex;
import java.util.Scanner;

public class bakjoon {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("몇번 반복 할래요?");
		int a =sc.nextInt();
		
		for (int i =1; i<=a;i++) {
			System.out.println("n,m 입력");
			int b = sc.nextInt();
			int c = sc.nextInt();
		
		}
		
	}
}
