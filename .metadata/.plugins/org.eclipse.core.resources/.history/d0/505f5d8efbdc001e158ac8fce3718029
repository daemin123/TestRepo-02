package Ch12;

// 매서드 오버라이딩 (함수 재정의)
// 상속 관계를 전제로 상위 클래스의 메서드를 하위 클래스가 재정의(고쳐서 사용)하는 것을 허용한 문법
// 


class Animal {
	void sound() {
		System.out.println("소리낸다");
	}
}
class Dog extends Animal {
	
}
class Cat extends Animal {
	
}
public class C03OverridingMain {
	public static void main(String[] args) {
	Dog a = new Dog();
	Dog.sound();
	}
}
