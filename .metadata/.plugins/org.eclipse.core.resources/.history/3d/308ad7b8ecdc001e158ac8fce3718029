package Ch12;

class Point2D {
	int x;
	int y;
	
}
class Point3D extends Point2D {
	int z;

	@Override
	public String toString() {
		return "Point3D [z=" + z + ", x=" + x + ", y=" + y + "]";
	}
	
}


public class C01InheritanceMain {
	public static void main(String[] args) {
		Point3D ob = new Point3D();
		ob.x = 100; 
		ob.y = 200;
		ob.z = 300;
		System.out.println(ob);
	}
}
